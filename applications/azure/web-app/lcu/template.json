{
    "$schema": "https://schema.lowcodeunit.com/schemas/latest/InfrastructureApplicationSeedOption.json#",
    "Name": "Low Code Unit",
    "Description": "Start creating a new Low Code Unit.",
    "ImageSource": "https://fathym.com/wp-content/uploads/2018/03/Fathym-Logo_Registered_xxsm.png",
    "SeedFork": null,
    "ReleasePackageSuffix": "-demo",
    "Commands": [
        "npm i @angular/cli @lcu/cli@latest -g",
        "npm i @lowcodeunit-devkit/ng-lcu --save-dev",
        "lcu init --workspace=$(RepoName) --scope=@$(RepoOrg)",
        "lcu proj lcu --template=LCU"
    ]
}
